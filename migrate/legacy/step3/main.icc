/* main.exe */

        group R_main_obj_ = 'main.cpp', 'payroll.hpp'
        group _main_obj_ = 'main.cpp'
        group D_main_obj_ = _main_obj_, R_main_obj_

        group R_main_exe_ = 'main.obj', 'payroll.lib'
        option compatibility_options,
